{
  "name": "openapi-nitro",
  "version": "0.1.10",
  "description": "Automatic generation of OpenAPI spec with Swagger and Scalar + type validation with Zod",
  "keywords": [
    "OpenAPI",
    "Swagger",
    "Scalar",
    "Typescript",
    "Zod",
    "Nitro"
  ],
  "author": "Magnus Bendix Borregaard <magnus.borregaard@gmail.com>",
  "private": false,
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "prepublishOnly": "bun run build",
    "dev": "nitro dev",
    "preview": "bun .output/server/index.mjs"
  },
  "devDependencies": {
    "@types/node": "^18.19.10",
    "nitropack": "latest",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0"
  },
  "peerDependencies": {
    "zod": "^3.23.8",
    "zod-openapi": "^2.19.0"
  },
  "dependencies": {
    "defu": "^6.1.4",
    "pathe": "^1.1.2",
    "ufo": "^1.3.2",
    "zod": "^3.23.8",
    "zod-openapi": "^2.19.0"
  }
}
